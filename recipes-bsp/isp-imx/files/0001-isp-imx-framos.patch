+    if (ret != 0) {
+        TRACE(IMX900_ERROR, "%s: set wb error\n", __func__);
+        return RET_FAILURE;
+    }
+
+    TRACE(IMX900_INFO, "%s: (exit)\n", __func__);
+
+    return RET_SUCCESS;
+}
