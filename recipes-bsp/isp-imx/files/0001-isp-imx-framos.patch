+    if (ret != 0) {
+        TRACE(IMX662_ERROR, "%s set sensor mode failed", __func__);
+        return RET_FAILURE;
+    }
+    memcpy(&pIMX662Ctx->CurMode, &sensor_mode, sizeof(struct vvcam_mode_info_s));
+    IMX662_UpdateIsiAEInfo(handle);
+
+    TRACE(IMX662_INFO, "%s (exit) \n", __func__);
+
+    return RET_SUCCESS;
