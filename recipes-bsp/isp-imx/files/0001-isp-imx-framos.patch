+static RESULT IMX900_IsiReleaseSensorIss(IsiSensorHandle_t handle)
+{
+    TRACE(IMX900_INFO, "%s (enter) \n", __func__);
+
+    IMX900_Context_t *pIMX900Ctx = (IMX900_Context_t *) handle;
+    if (pIMX900Ctx == NULL)
+        return (RET_WRONG_HANDLE);
+
+    IMX900_IsiSensorSetStreamingIss(pIMX900Ctx, BOOL_FALSE);
+    struct vvcam_clk_s clk;
+    memset(&clk, 0, sizeof(struct vvcam_clk_s));
+    IMX900_IsiSensorGetClkIss(pIMX900Ctx, &clk);
+    clk.status = 0;
+    IMX900_IsiSensorSetClkIss(pIMX900Ctx, &clk);
+    IMX900_IsiSensorSetPowerIss(pIMX900Ctx, BOOL_FALSE);
+    free(pIMX900Ctx);
+    pIMX900Ctx = NULL;
+
+    TRACE(IMX900_INFO, "%s (exit)\n", __func__);
+
+    return RET_SUCCESS;
+}
