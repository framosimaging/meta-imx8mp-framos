+static RESULT IMX900_UpdateIsiAEInfo(IsiSensorHandle_t handle)
+{
+    IMX900_Context_t *pIMX900Ctx = (IMX900_Context_t *) handle;
+
+    IsiSensorAeInfo_t *pAeInfo = &pIMX900Ctx->AeInfo;
+    pAeInfo->oneLineExpTime = pIMX900Ctx->CurMode.ae_info.one_line_exp_time_ns / 1000;
+
+    if (pIMX900Ctx->CurMode.hdr_mode == SENSOR_MODE_LINEAR) {
+        pAeInfo->maxIntTime.linearInt =
+            pIMX900Ctx->CurMode.ae_info.max_integration_line * pAeInfo->oneLineExpTime * 1000;
+        pAeInfo->minIntTime.linearInt =
+            pIMX900Ctx->CurMode.ae_info.min_integration_line * pAeInfo->oneLineExpTime * 1000;
+        pAeInfo->maxAGain.linearGainParas = pIMX900Ctx->CurMode.ae_info.max_again;
+        pAeInfo->minAGain.linearGainParas = pIMX900Ctx->CurMode.ae_info.min_again;
+        pAeInfo->maxDGain.linearGainParas = pIMX900Ctx->CurMode.ae_info.max_dgain;
+        pAeInfo->minDGain.linearGainParas = pIMX900Ctx->CurMode.ae_info.min_dgain;
+    } else {
+        switch (pIMX900Ctx->CurMode.stitching_mode) {
+            case SENSOR_STITCHING_DUAL_DCG:
+            case SENSOR_STITCHING_3DOL:
+            case SENSOR_STITCHING_LINEBYLINE:
+                pAeInfo->maxIntTime.triInt.triSIntTime =
+                    pIMX900Ctx->CurMode.ae_info.max_vsintegration_line * pAeInfo->oneLineExpTime;
+                pAeInfo->minIntTime.triInt.triSIntTime =
+                    pIMX900Ctx->CurMode.ae_info.min_vsintegration_line * pAeInfo->oneLineExpTime;
+                
+                pAeInfo->maxIntTime.triInt.triIntTime =
+                    pIMX900Ctx->CurMode.ae_info.max_integration_line * pAeInfo->oneLineExpTime;
+                pAeInfo->minIntTime.triInt.triIntTime =
+                    pIMX900Ctx->CurMode.ae_info.min_integration_line * pAeInfo->oneLineExpTime;
+
+                if (pIMX900Ctx->CurMode.stitching_mode == SENSOR_STITCHING_DUAL_DCG) {
+                    pAeInfo->maxIntTime.triInt.triLIntTime = pAeInfo->maxIntTime.triInt.triIntTime;
+                    pAeInfo->minIntTime.triInt.triLIntTime = pAeInfo->minIntTime.triInt.triIntTime;
+                } else {
+                    pAeInfo->maxIntTime.triInt.triLIntTime =
+                        pIMX900Ctx->CurMode.ae_info.max_longintegration_line * pAeInfo->oneLineExpTime;
+                    pAeInfo->minIntTime.triInt.triLIntTime =
+                        pIMX900Ctx->CurMode.ae_info.min_longintegration_line * pAeInfo->oneLineExpTime;
+                }
+
+                pAeInfo->maxAGain.triGainParas.triSGain = pIMX900Ctx->CurMode.ae_info.max_short_again;
+                pAeInfo->minAGain.triGainParas.triSGain = pIMX900Ctx->CurMode.ae_info.min_short_again;
+                pAeInfo->maxDGain.triGainParas.triSGain = pIMX900Ctx->CurMode.ae_info.max_short_dgain;
+                pAeInfo->minDGain.triGainParas.triSGain = pIMX900Ctx->CurMode.ae_info.min_short_dgain;
+
+                pAeInfo->maxAGain.triGainParas.triGain = pIMX900Ctx->CurMode.ae_info.max_again;
+                pAeInfo->minAGain.triGainParas.triGain = pIMX900Ctx->CurMode.ae_info.min_again;
+                pAeInfo->maxDGain.triGainParas.triGain = pIMX900Ctx->CurMode.ae_info.max_dgain;
+                pAeInfo->minDGain.triGainParas.triGain = pIMX900Ctx->CurMode.ae_info.min_dgain;
+
+                pAeInfo->maxAGain.triGainParas.triLGain = pIMX900Ctx->CurMode.ae_info.max_long_again;
+                pAeInfo->minAGain.triGainParas.triLGain = pIMX900Ctx->CurMode.ae_info.min_long_again;
+                pAeInfo->maxDGain.triGainParas.triLGain = pIMX900Ctx->CurMode.ae_info.max_long_dgain;
+                pAeInfo->minDGain.triGainParas.triLGain = pIMX900Ctx->CurMode.ae_info.min_long_dgain;
+                break;
+            case SENSOR_STITCHING_DUAL_DCG_NOWAIT:
+            case SENSOR_STITCHING_16BIT_COMPRESS:
+            case SENSOR_STITCHING_L_AND_S:
+            case SENSOR_STITCHING_2DOL:
+                pAeInfo->maxIntTime.dualInt.dualIntTime =
+                    pIMX900Ctx->CurMode.ae_info.max_integration_line * pAeInfo->oneLineExpTime;
+                pAeInfo->minIntTime.dualInt.dualIntTime =
+                    pIMX900Ctx->CurMode.ae_info.min_integration_line * pAeInfo->oneLineExpTime;
+
+                if (pIMX900Ctx->CurMode.stitching_mode == SENSOR_STITCHING_DUAL_DCG_NOWAIT) {
+                    pAeInfo->maxIntTime.dualInt.dualSIntTime = pAeInfo->maxIntTime.dualInt.dualIntTime;
+                    pAeInfo->minIntTime.dualInt.dualSIntTime = pAeInfo->minIntTime.dualInt.dualIntTime;
+                } else {
+                    pAeInfo->maxIntTime.dualInt.dualSIntTime =
+                        pIMX900Ctx->CurMode.ae_info.max_vsintegration_line * pAeInfo->oneLineExpTime;
+                    pAeInfo->minIntTime.dualInt.dualSIntTime =
+                        pIMX900Ctx->CurMode.ae_info.min_vsintegration_line * pAeInfo->oneLineExpTime;
+                }
+                
+                if (pIMX900Ctx->CurMode.stitching_mode == SENSOR_STITCHING_DUAL_DCG_NOWAIT) {
+                    pAeInfo->maxAGain.dualGainParas.dualSGain = pIMX900Ctx->CurMode.ae_info.max_again;
+                    pAeInfo->minAGain.dualGainParas.dualSGain = pIMX900Ctx->CurMode.ae_info.min_again;
+                    pAeInfo->maxDGain.dualGainParas.dualSGain = pIMX900Ctx->CurMode.ae_info.max_dgain;
+                    pAeInfo->minDGain.dualGainParas.dualSGain = pIMX900Ctx->CurMode.ae_info.min_dgain;
+                    pAeInfo->maxAGain.dualGainParas.dualGain  = pIMX900Ctx->CurMode.ae_info.max_long_again;
+                    pAeInfo->minAGain.dualGainParas.dualGain  = pIMX900Ctx->CurMode.ae_info.min_long_again;
+                    pAeInfo->maxDGain.dualGainParas.dualGain  = pIMX900Ctx->CurMode.ae_info.max_long_dgain;
+                    pAeInfo->minDGain.dualGainParas.dualGain  = pIMX900Ctx->CurMode.ae_info.min_long_dgain;
+                } else {
+                    pAeInfo->maxAGain.dualGainParas.dualSGain = pIMX900Ctx->CurMode.ae_info.max_short_again;
+                    pAeInfo->minAGain.dualGainParas.dualSGain = pIMX900Ctx->CurMode.ae_info.min_short_again;
+                    pAeInfo->maxDGain.dualGainParas.dualSGain = pIMX900Ctx->CurMode.ae_info.max_short_dgain;
+                    pAeInfo->minDGain.dualGainParas.dualSGain = pIMX900Ctx->CurMode.ae_info.min_short_dgain;
+                    pAeInfo->maxAGain.dualGainParas.dualGain  = pIMX900Ctx->CurMode.ae_info.max_again;
+                    pAeInfo->minAGain.dualGainParas.dualGain  = pIMX900Ctx->CurMode.ae_info.min_again;
+                    pAeInfo->maxDGain.dualGainParas.dualGain  = pIMX900Ctx->CurMode.ae_info.max_dgain;
+                    pAeInfo->minDGain.dualGainParas.dualGain  = pIMX900Ctx->CurMode.ae_info.min_dgain;
+                }
+                
+                break;
+            default:
+                break;
+        }
+    }
+    pAeInfo->gainStep = pIMX900Ctx->CurMode.ae_info.gain_step;
+    pAeInfo->currFps  = pIMX900Ctx->CurMode.ae_info.cur_fps;
+    pAeInfo->maxFps   = pIMX900Ctx->CurMode.ae_info.max_fps;
+    pAeInfo->minFps   = pIMX900Ctx->CurMode.ae_info.min_fps;
+    pAeInfo->minAfps  = pIMX900Ctx->CurMode.ae_info.min_afps;
+    pAeInfo->hdrRatio[0] = pIMX900Ctx->CurMode.ae_info.hdr_ratio.ratio_l_s;
+    pAeInfo->hdrRatio[1] = pIMX900Ctx->CurMode.ae_info.hdr_ratio.ratio_s_vs;
+
+    pAeInfo->intUpdateDlyFrm = pIMX900Ctx->CurMode.ae_info.int_update_delay_frm;
+    pAeInfo->gainUpdateDlyFrm = pIMX900Ctx->CurMode.ae_info.gain_update_delay_frm;
+
+    if (pIMX900Ctx->minAfps != 0) {
+        pAeInfo->minAfps = pIMX900Ctx->minAfps;
+    } 
+    return RET_SUCCESS;
+}
