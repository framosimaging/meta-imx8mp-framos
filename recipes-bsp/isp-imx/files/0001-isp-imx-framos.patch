+static RESULT IMX900_IsiCreateSensorIss(IsiSensorInstanceConfig_t * pConfig)
+{
+    RESULT result = RET_SUCCESS;
+    IMX900_Context_t *pIMX900Ctx;
+
+    TRACE(IMX900_INFO, "%s (enter)\n", __func__);
+
+    if (!pConfig || !pConfig->pSensor || !pConfig->HalHandle)
+        return RET_NULL_POINTER;
+
+    pIMX900Ctx = (IMX900_Context_t *) malloc(sizeof(IMX900_Context_t));
+    if (!pIMX900Ctx)
+        return RET_OUTOFMEM;
+
+    memset(pIMX900Ctx, 0, sizeof(IMX900_Context_t));
+    pIMX900Ctx->IsiCtx.HalHandle = pConfig->HalHandle;
+    pIMX900Ctx->IsiCtx.pSensor   = pConfig->pSensor;
+    pConfig->hSensor = (IsiSensorHandle_t) pIMX900Ctx;
+
+    result = IMX900_IsiSensorSetPowerIss(pIMX900Ctx, BOOL_TRUE);
+    if (result != RET_SUCCESS) {
+        TRACE(IMX900_ERROR, "%s set power error\n", __func__);
+        return RET_FAILURE;
+    }
+    struct vvcam_clk_s clk;
+    memset(&clk, 0, sizeof(struct vvcam_clk_s));
+    result = IMX900_IsiSensorGetClkIss(pIMX900Ctx, &clk);
+    if (result != RET_SUCCESS) {
+        TRACE(IMX900_ERROR, "%s get clk error\n", __func__);
+        return RET_FAILURE;
+    }
+    clk.status = 1;
+    result = IMX900_IsiSensorSetClkIss(pIMX900Ctx, &clk);
+    if (result != RET_SUCCESS) {
+        TRACE(IMX900_ERROR, "%s set clk error\n", __func__);
+        return RET_FAILURE;
+    }
+    result = IMX900_IsiResetSensorIss(pIMX900Ctx);
+    if (result != RET_SUCCESS) {
+        TRACE(IMX900_ERROR, "%s retset sensor error\n", __func__);
+        return RET_FAILURE;
+    }
+
+    IsiSensorMode_t SensorMode;
+    SensorMode.index = pConfig->SensorModeIndex;
+    result = IMX900_IsiSetSensorModeIss(pIMX900Ctx, &SensorMode);
+    if (result != RET_SUCCESS) {
+        TRACE(IMX900_ERROR, "%s set sensor mode error\n", __func__);
+        return RET_FAILURE;
+    }
+
+    TRACE(IMX900_INFO, "%s (exit)\n", __func__);
+
+    return result;
+}
